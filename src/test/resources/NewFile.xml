<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="suit">
	<test name="test">
	<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="TestNG_Basics.WebDriverFactory">
				<methods>
				<include name="Fblanding"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="Girish" enabled="false">
		<classes>
			<class name="TestNG_Basics.WebDriverFactory">
				<methods>
				<include name="Googlelanding"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>